<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>慕云游</title>
  </head>
  <body>
    <header class="site-head">
      <!-- topbar -->
      {{include "../../components/topbar/topbar.art"}}

      <!-- main-nav -->
      {{include "../../components/main-nav/main_nav.art"}}

      <!-- header-con -->
      {{include "../../components/header-con/header_con.art"}}
    </header>

    <section class="banner" id="banner">
      <!-- carousel -->
      <div id="slider-layout">
      <% include("../../components/loading/loading.art",{inline: true}) %>
      </div>

      <div class="center-wrap">
      <!--btns -->
      {{include "./components/slider/btn.art"}}
      
      <!-- menu-box -->
        <div class="v-menu-box" id="v-menu-box">
          <nav class="v-menu" id="v-menu">
            <% include("../../components/loading/loading.art",{inline: true}) %>
          </nav>
        </div>
      
      </div>
    </section>

    <!-- fav-ad -->
    <section class="layer fav-ad">
      <div class="center-wrap">
        <% include("../../components/loading/loading.art",{inline: true}) %>
      </div>
    </section>

    <!-- xxsw -->
    <section class="layer xxsw">
      <div class="center-wrap">
        {{include "./components/xxsw/xxsw.art"}}
      </div>
    </section>

    <!-- jjzyx -->
    <section class="layer layer-common-style jjzyx">
      <div class="center-wrap">
        {{include "./components/jjzyx/jjzyx.art"}}
      </div>
    </section>

    <!-- ddwl -->
    <section class="layer ddwl">
      <div class="center-wrap">
        {{include "./components/ddwl/ddwl.art"}}
      </div>
    </section>

    <!-- tsddty -->
    <section class="layer tsddty">
      <div class="center-wrap">
        {{include "./components/tsddty/tsddty.art"}}
      </div>
    </section>

    <!-- zsj -->
    <section class="layer layer-common-style another-style zsj">
      <div class="center-wrap">
        {{include "./components/zsj/zsj.art"}}
      </div>
    </section>

    <!-- gt -->
    <section class="layer layer-common-style another-style gt">
      <div class="center-wrap">
        {{include "./components/gty/gty.art"}}
      </div>
    </section>

    <footer>
      <!-- footer-nav -->
      {{include "../../components/footer-nav/footer_nav.art"}}

      <!-- bottom-bar -->
      {{include "../../components/bottom-bar/bottom_bar.art"}}

      <!-- backtotop -->
      {{include "../../components/backtotop/backtotop.art"}}
    </footer>
  </body>
</html>
